{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "entry",
        "corejs": "3.0.0"
      }
    ]
  ],
  "plugins": [
    "@babel/transform-runtime"
  ],
  "env": {
    "cjs": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs"
      ]
    }
  }
}
